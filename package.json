{
  "name": "bus",
  "version": "1.0.0",
  "description": "Bus pour microservice",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently -k -n \"Typescript,NodeJS\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/index.js\"",
    "test": "nyc --reporter=html --reporter=text mocha -r ts-node/register tests/*.ts",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [
    "Bus",
    "microservice"
  ],
  "author": "Cyril POIDEVIN <vilsafur@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.5",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "express": "^4.17.1",
    "uuid": "^3.3.2"
  }
}
